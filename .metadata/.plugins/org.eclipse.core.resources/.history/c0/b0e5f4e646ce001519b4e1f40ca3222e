class AltaUsuarioController < ApplicationController

  def index
    
    if @agen.is_a?(Listin) 
       if params[:boton] == "Alta"
         puts "Estamos aqui"
         @agen.alta params[:nombre],params[:apellidos],params[:edad],params[:telefono]
       end
    else
     @agen = Listin.new
    end
    @listausuarios= @agen.lista
  end
  
end
