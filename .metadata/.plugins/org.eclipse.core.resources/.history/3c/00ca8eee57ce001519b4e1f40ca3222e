class Listin
  def initialize
    puts "inicializando"
  end
  
    def alta nombre,apellidos,edad,telefono
    usuario1= Usuario.new nombre,apellidos,edad,telefono
    Dao.alta usuario1
  end
  
  def lista
    @lista = Dao.listar
    @lista
  end
  
  def byId id
    Dao.getClientesById id

  end
end